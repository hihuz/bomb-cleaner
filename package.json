{
  "name": "bomb-cleaner",
  "version": "1.0.0",
  "description": "mine sweeper clone built with react and redux",
  "main": "index.js",
  "scripts": {
    "lint": "eslint js/**/*.js webpack.config.js",
    "fix-lint": "eslint --fix js/**/*.js webpack.config.js",
    "build": "webpack -p --env.prod",
    "watch": "npm run build -- --watch",
    "dev": "webpack-dev-server --env.dev",
    "test": "jest",
    "update-test": "jest -u",
    "cover": "npm run test -- --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2017": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.3.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "jsdom": "^9.9.1",
    "json-loader": "0.5.4",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.25",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "1.16.2",
    "webpack-validator": "^2.3.0"
  },
  "author": "Sylvain",
  "license": "MIT"
}